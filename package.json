{
  "name": "solito-blank",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/react": "^17.0.39",
    "@types/react-native": "^0.67.2",
    "eslint": "^8.11.0",
    "turbo": "^1.1.6",
    "typescript": "^4.6.2",
    "prettier": "^2.5.1"
  },
  "scripts": {
    "native": "cd apps/expo && expo start",
    "web": "cd apps/next && yarn next",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "update-db": "cd packages/db && npm run update",
    "clean": "find . -name node_modules -o -name .next -o -name .expo -o -name .turbo -type d -prune | xargs rm -rf"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  }
}
